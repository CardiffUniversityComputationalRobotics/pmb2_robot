<launch>
  <arg name="robot" default="full"/>

  <!-- Add casters as joint_state_controller extra joints -->
  <rosparam command="load"
            ns="joint_state_controller"
            file="$(find pmb2_controller_configuration)/config/joint_state_controller_extra_joints.yaml"/>

  <!-- Diff drive controller multipliers -->
  <rosparam command="load" file="$(find pmb2_controller_configuration)/config/mobile_base_controller_multipliers.yaml"/>

  <!-- pmb2 hardware -->
  <rosparam command="load" file="$(find pmb2_bringup)/config/pmb2_hardware.yaml"/>

  <!-- Robot model -->
  <include file="$(find pmb2_description)/robots/upload_pmb2.launch">
    <arg name="robot" value="$(arg robot)"/>
  </include>

  <!-- Bring up robot -->
  <include file="$(find pmb2_bringup)/launch/pmb2_bringup.launch"/>

  <!-- Bumper, IR and Sonar to cloud -->
  <!-- @todo use custom config (requires changes in sensor_to_cloud -->
  <!-- @todo no bumper, IR or sonar yet -->
  <!--include file="$(find sensor_to_cloud)/launch/bumper_to_cloud.launch"/>
    <arg name="config" value="$(find pmb2_bringup)/config/sensor_to_cloud/bumper_to_cloud.yaml"/>
  </include>
  <include file="$(find sensor_to_cloud)/launch/sonar_to_cloud.launch"/>
    <arg name="config" value="$(find pmb2_bringup)/config/sensor_to_cloud/sonar_to_cloud.yaml"/>
  </include-->

</launch>
